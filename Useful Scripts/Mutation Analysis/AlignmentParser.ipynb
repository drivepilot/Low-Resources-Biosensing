    "            gCounter = gCounter + 0.5\n",
    "        elif c == \"w\":\n",
    "            aCounter = aCounter + 0.5\n",
    "            tCounter = tCounter + 0.5\n",
    "        elif c == \"k\":\n",
    "            tCounter = tCounter + 0.5\n",
    "            gCounter = gCounter + 0.5\n",
    "        elif c == \"m\":\n",
