    "    global gCounter\n",
    "    global cCounter\n",
    "    global pCounter\n",
    "    \n",
    "    if c == \"a\":\n",
    "        aCounter = aCounter + 1\n",
    "    elif c == \"t\":\n",
    "        tCounter = tCounter + 1\n",
    "    elif c == \"g\":\n",
    "        gCounter = gCounter + 1\n",
    "    elif c == \"c\":\n",
    "        cCounter = cCounter + 1\n",
    "    elif c == \"-\":\n",
    "        pCounter = pCounter + 1\n",
    "    elif degeneratedNucleotides:\n",
    "        if c == \"r\":\n",
